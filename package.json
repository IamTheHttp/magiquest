{
  "name": "magiquest",
  "private": true,
  "version": "0.0.1",
  "description": "Magic Quest Game!",
  "scripts": {
    "build": "rm -rf ./dist && rollup -c",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "rm -rf ./dist && rollup -c -w & live-server ./dist",
    "release": "release-it",
    "formatcode": "prettier --write --print-width 120 --single-quote --no-bracket-spacing --trailing-comma none src __tests__"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write . && git add -A . && npm run test"
    }
  },
  "dependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-image": "^2.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-url": "^6.1.0",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "bootstrap": "^3.3.7",
    "canvas": "^2.8.0",
    "core-js": "3.6.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "game-platform": "^3.0.0",
    "generate-source-map": "0.0.5",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
    "jest-canvas-mock": "2.1.1",
    "live-server": "^1.2.1",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "rollup": "^2.56.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-require-context": "^1.0.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sass": "^1.37.5",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0"
  },
  "devDependencies": {
    "@web/dev-server": "^0.1.28",
    "prettier": "^2.5.1"
  }
}
