{
  "name": "magiquest",
  "private": true,
  "version": "0.0.1",
  "description": "Magic Quest Game!",
  "scripts": {
    "start": "webpack-dev-server --open --mode development --hot",
    "build": "webpack --mode production",
    "test": "jest --maxWorkers=2",
    "test:watch": "npm run test -- --watch",
    "test:commit": "npm run build && npm run test",
    "test:coverage": "npm run test -- --coverage",
    "start:editor": "cd Editor; npm start",
    "release": "release-it",
    "lint": "eslint . --fix",
    "formatcode": "prettier --write src __tests__ Editor *.js *.json"
  },
  "dependencies": {
    "babel-loader": "9.1.3",
    "bootstrap": "5.3.3",
    "canvas": "2.11.2",
    "core-js": "3.35.1",
    "game-platform": "^3.1.5",
    "generate-source-map": "0.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@babel/core": "7.24.4",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "7.24.4",
    "@babel/preset-react": "7.24.1",
    "@types/eslint": "8.56.3",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@web/dev-server": "0.4.1",
    "css-loader": "7.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-canvas-mock": "2.5.2",
    "jest-environment-jsdom": "29.7.0",
    "json-loader": "0.5.7",
    "live-server": "1.2.2",
    "prettier": "3.2.5",
    "release-it": "17.2.1",
    "sass": "1.75.0",
    "sass-loader": "14.1.1",
    "style-loader": "4.0.0",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.3.3",
