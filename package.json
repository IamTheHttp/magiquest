{
  "name": "magiquest",
  "private": true,
  "version": "0.0.1",
  "description": "Magic Quest Game!",
  "scripts": {
    "init": "npm install && jarb init",
    "start": "npm run build:data && jarb devServer --mode=development",
    "build:dev": "npm run build:data && jarb build --mode=development",
    "build:data": "ts-node ./tools/buildData.ts",
    "build": "npm run build:data && jarb build",
    "analyze": "jarb analyze --analyzeBundle",
    "test": "npm run build:data && jarb test --coverage",
    "tdd": "npm run build:data && jarb test",
    "release": "jarb release"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/webpack-env": "^1.15.1",
    "jarb": "4.0.0-beta.4",
    "jest-canvas-mock": "2.1.1",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/enzyme": "^3.10.5",
    "canvas": "2.6.0",
    "core-js": "3.6.4",
    "csvtojson": "^2.0.10",
    "game-platform": "1.0.1",
    "react": "16.8.2",
    "react-dom": "16.8.2"
  },
  "main": "dist",
  "jsnext:main": "src",
  "module": "src"
}
