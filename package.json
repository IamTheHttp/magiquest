{
  "name": "magiquest",
  "private": true,
  "version": "0.0.1",
  "description": "Magic Quest Game!",
  "scripts": {
    "start": "webpack-dev-server --open --mode development --hot",
    "build": "webpack --mode production",
    "test": "jest --maxWorkers=2",
    "test:watch": "npm run test -- --watch",
    "test:commit": "npm run build && npm run test",
    "test:coverage": "npm run test -- --coverage",
    "start:editor": "cd Editor; npm start",
    "release": "release-it",
    "lint": "eslint . --fix",
    "formatcode": "prettier --write src __tests__ Editor *.js *.json"
  },
  "dependencies": {
    "babel-loader": "9.1.3",
    "bootstrap": "5.2.3",
    "canvas": "2.11.2",
    "core-js": "3.30.2",
    "game-platform": "^3.1.5",
    "generate-source-map": "0.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@babel/core": "7.22.1",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "7.22.2",
    "@babel/preset-react": "7.22.0",
    "@types/eslint": "8.44.6",
    "@types/jest": "29.5.1",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^5.59.7",
    "@web/dev-server": "0.2.1",
    "css-loader": "6.8.1",
    "eslint": "^8.52.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-canvas-mock": "2.5.1",
    "jest-environment-jsdom": "29.7.0",
    "json-loader": "0.5.7",
    "live-server": "1.2.2",
    "prettier": "2.8.8",
    "release-it": "15.10.3",
    "sass": "1.62.1",
    "sass-loader": "13.3.0",
    "style-loader": "3.3.3",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.0.4",
