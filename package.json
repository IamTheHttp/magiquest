{
  "name": "magiquest",
  "private": true,
  "version": "0.0.1",
  "description": "Magic Quest Game!",
  "scripts": {
    "start": "webpack-dev-server --open --mode development --hot",
    "build": "webpack --mode production",
    "test": "jest --maxWorkers=2",
    "test:watch": "npm run test -- --watch",
    "test:commit": "npm run build && npm run test",
    "test:coverage": "npm run test -- --coverage",
    "start:editor": "cd Editor; npm start",
    "release": "release-it",
    "formatcode": "prettier --write --print-width 120 --single-quote --no-bracket-spacing --trailing-comma none src __tests__ Editor *.js *.json"
  },
  "dependencies": {
    "babel-loader": "^8.2.5",
    "bootstrap": "^5.2.0",
    "canvas": "^2.9.3",
    "core-js": "^3.25.0",
    "game-platform": "^3.1.2",
    "generate-source-map": "0.0.5",
    "html-webpack-plugin": "^5.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^15.6.0",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack-cli": "^4.10.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@web/dev-server": "^0.1.34",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "json-loader": "^0.5.7",
    "prettier": "^2.8.3",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "webpack": "^5.74.0",
    "webpack-dev-server": "^4.11.1",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "live-server": "^1.2.2"
  }
}
